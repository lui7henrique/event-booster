{
  "name": "event-booster-server",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/http/server.ts",
    "db:seed": "tsx --env-file=.env src/db/seed.ts",
    "db:generate": "dotenv -e .env -- drizzle-kit generate",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:migrate:test": "dotenv -e .env.test -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env -- drizzle-kit studio",
    "pretest": "npm run db:migrate:test",
    "test": "dotenv -e .env.test -- vitest",
    "test:coverage": "dotenv -e .env.test -- vitest --coverage",
    "test:ui": "dotenv -e .env.test -- vitest --ui",
    "format": "biome check src --write",
    "mail:dev": "email dev --dir src/mail/templates",
    "build": "dotenv -e .env -- tsup src/http/server.ts --out-dir dist --format esm,cjs --dts",
    "start": "dotenv -e .env -- node dist/server.js",
    "teste": "npx ts-node --require ./instrumentation.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.1",
    "@fastify/rate-limit": "^10.0.1",
    "@fastify/redis": "^7.0.1",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.2",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/sdk-metrics": "^1.26.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-node": "^1.26.0",
    "@sentry/node": "^8.34.0",
    "@sentry/profiling-node": "^8.34.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "0.33.0",
    "drizzle-zod": "^0.5.1",
    "fastify": "^5.0.0",
    "fastify-type-provider-zod": "^4.0.1",
    "postgres": "3.4.4",
    "typeid-js": "1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@faker-js/faker": "8.4.1",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "22.5.0",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "dotenv-cli": "7.4.2",
    "drizzle-kit": "0.24.1",
    "tsup": "8.2.4",
    "tsx": "4.17.0",
    "typescript": "5.5.4",
    "vite-tsconfig-paths": "5.0.1",
    "vitest": "^2.1.1",
    "zod": "3.23.8"
  }
}